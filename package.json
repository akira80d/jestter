{
  "name": "jestter",
  "version": "0.1.8",
  "description": "STUB FIRST DEVELOPMENT. Jestter is intended to aid Test for Jest.",
  "main": "bin/jestter/index.js",
  "bin": {
    "jestter": "bin/jestter/index.js"
  },
  "type": "module",
  "files": [
    "bin",
    "lib",
    "data"
  ],
  "directories": {
    "bin": "./bin",
    "lib": "./lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akira80d/jestter.git"
  },
  "auther": "akira80d",
  "license": "MIT",
  "keywords": [
    "javaScript",
    "jest",
    "test",
    "stub"
  ],
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "babel-plugin-rewire": "^1.2.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^27.4.5",
    "prettier": "2.5.1",
    "typescript": "^4.5.4",
    "watch": "^0.13.0"
  },
  "scripts": {
    "build": "npm run tsc",
    "prepublishOnly": "npm run build",
