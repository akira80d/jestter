{
  "name": "jestter",
  "version": "0.1.3",
  "description": "STUB FIRST. Jestter scaffold Jest Test File from your stub.",
  "main": "bin/jestter/index.js",
  "bin": {
    "jestter": "bin/jestter/index.js"
  },
  "type": "module",
  "directories": {
    "bin": "./bin",
    "lib": "./lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akira80d/jestter.git"
  },
  "auther": "akira80d",
  "license": "MIT",
  "keywords": [
    "javaScript",
    "jest",
    "test",
    "stub"
  ],
  "devDependencies": {
    "jest": "^27.4.5",
    "prettier": "2.5.1",
    "watch": "^0.13.0"
  },
  "scripts": {
    "build": "npm run babel:bin & npm run babel:lib",
    "prepublishOnly": "npm run build",
    "jestter": "node ./bin/jestter/index.js",
    "watch": "npm run watch:bin & npm run watch:lib",
    "watch:bin": "watch 'npm run babel:bin' src/bin/",
    "watch:lib": "watch 'npm run babel:lib' src/lib/",
    "babel:bin": "./node_modules/.bin/babel src/bin/ --out-dir bin/",
    "babel:lib": "./node_modules/.bin/babel src/lib/ --out-dir lib/",
    "test": "node --experimental-vm-modules node_modules/.bin/jest"
  },
  "dependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5"
  },
  "bugs": {
    "url": "https://github.com/akira80d/jestter/issues"
  },
  "homepage": "https://github.com/akira80d/jestter#readme"
}
