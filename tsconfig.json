{
  "compilerOptions": {
    "target": "es6",
    "module": "esnext",
    "declaration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "rootDir": "./src",
    "outDir": "",
    "moduleResolution": "node"
  },
  "exclude": [
	  "node_modules",
	  "*.test.ts",
	  "*.d.ts",
	  "./bin",
	  "./lib",
    "testdata"
  ]
}
